restart;
 GaussianQuadrature := proc(n)
    local Pn, dPn, roots, weights, k, t, P;
    P := Array(1..n);
    P[1] := 1; P[2] := t;
    for k from 2 to n do
        P[k+1] := expand((2*k+1)/k*t*P[k] - (k)/(k+1)*P[k-1]);
    end do;
    Pn := P[n];
    dPn := diff(Pn, t);
    roots := [fsolve(Pn = 0, t = -1..1)];
    weights := Array(1..n);
    for k to n do
        weights[k] := 2 / ((1 - roots[k]^2) * (eval(dPn, t = 
roots[k]))^2);
    end do;
    return [roots, weights];
 end proc</Text-field>
</Input>
